    "nav-frontend-ikonknapper": "0.0.18",
    "nav-frontend-js-utils": "^1.0.13",
    "nav-frontend-knapper": "^2.0.22",
    "nav-frontend-knapper-style": "^0.3.38",
    "nav-frontend-lenker": "^1.0.41",
    "nav-frontend-lenker-style": "^0.2.29",
    "nav-frontend-lukknapp": "^1.0.31",
